{
    "name": "makhnanov/php-telegram-bot",
    "description": "PHP 8.1+ Telegram Bot API Library.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Makhnanov\\Telegram81\\": "Src/"
        },
        "files": ["Src/Function.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Makhnanov\\Telegram81\\Test\\": "Test/"
        }
    },
    "authors": [
        {
            "name": "Romaxa",
            "email": "bakirov.important@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "7.4.x-dev",
        "yiisoft/arrays": "dev-master",
        "makhnanov/php-enum": "dev-main"
    },
    "require-dev": {
        "symfony/var-dumper": "6.0.x-dev",
        "phpunit/phpunit": "^9",
        "ezyang/htmlpurifier": "4.13"
    },
    "scripts": {
        "test": "php vendor/bin/phpunit --colors=always --testdox Test"
    }
}
